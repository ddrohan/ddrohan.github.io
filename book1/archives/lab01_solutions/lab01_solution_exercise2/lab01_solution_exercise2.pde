//Setting up the display window
size(200,325);
background(255); 
strokeWeight(2);

ellipse(100,50,60,60);    //drawing the head
line(100,80,100,200);     //drawing the body
line(100,200,50,260);     //drawing the left leg
line(100,200,150,260);    //drawing the right leg
line(50,120,100,150);     //drawing the left arm
line(150,120,100,150);    //drawing the right arm